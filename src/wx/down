		// and now our improved toggler!
		function toggle(obj,image) {
			var el = $(obj);
			el.style.display = (el.style.display != 'none' ? 'none' : '' );
			img=$(image);
			img.src= (img.src!="http://v4.vcimg.com/images/v3/toggle.gif"?"http://v4.vcimg.com/images/v3/toggle.gif":"http://v4.vcimg.com/images/v3/hide.gif");
			img.style.marginTop=(img.style.marginTop!="-28px"?"-28px":"-26px");
		}
        if($("closeUser")){
            $("closeUser").onclick=function(){
                toggle("userMore","closeUser");
            }
        }
        if($("closeRel")){
            $("closeRel").onclick=function(){
                toggle("relMore","closeRel");
            }
        }
        if($("closeRell")){
            $("closeRell").onclick=function(){
                toggle("closeRellmore","closeRell");
