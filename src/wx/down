		new Ajax.Request ('http://www.verycd.com/ajax/folder?m=get_comments&id=2788317', {
        method:'get',
        onComplete: function(transport){
        	if (transport.status==200){
				$('userCommentDiv').innerHTML=transport.responseText;
				dynamicTime.updatePage();
				setTimeout(memberHonor.updatePage(),100);
			}else{
				$('commentLoading').innerHTML = '';
			}
		}
		});
		return true;
	}
	setTimeout(onLoadComment,100);
}
onLoadComment();

