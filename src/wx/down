<script type="text/javascript">
function feedback(commentId, floor) {
    var msg;
    msg ="http://www.verycd.com/topics/2788317/comments/comment"+commentId;
    msg += "\n";

    if(-1 != floor) {
        msg += "举报: 第"+floor+"楼\n";
    } else {
        msg += "举报:热门评论\n";
    }
    msg += "原因: \n";
    $('feedback_contents').value = msg;
    $('feedback_contents').name  = 'contents';
    $('feedback_form').submit();
    $('feedback_contents').name  = '';
}
</script>

