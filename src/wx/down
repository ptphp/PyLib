                    <script>
                    function show_favorite_tips(){
                        if ( VeryCD.cookie.get('favoritefolder') ){
                            document.getElementById("folderfavoritatips").style.display="none";
                        } else {
                            document.getElementById("folderfavoritatips").style.display="";
                            var googletotal = function(){
                                try{
                                    VeryCD.Track('/stat/topicfavtips/');
                                }catch(e){
                                    setTimeout(googletotal,100);
                                }
                            };
                            googletotal();
                        }
                    }
                    function hidden_favorite_tips(){
                        var Then = new Date();
                        Then.setFullYear(Then.getFullYear() + 2);
                        VeryCD.cookie.set('favoritefolder','1',Then,'/', '.verycd.com');
                        document.getElementById("folderfavoritatips").style.display="none";
                        return false;
                    }
                    </script>

                    <script type="text/javascript">
                    function collect_show_dialog(status, score){

                        if($('entryid') == undefined){
                            return false;
                        }

                        if (!status) return false;
                        var entryid = $('entryid').value;
                        var thisUrl = '/base/api/member/collection?entry_id='+entryid;
                          new Ajax.Request (thisUrl, {
                              method:'get',
                              requestHeaders: ['Cache-Control','no-cache','If-Modified-Since','0'],
                              onComplete: function(transport1) {
                                  var re = YAHOO.lang.JSON.parse(transport1.responseText);
                                  if(re.error != -1){
                                   var wishinput = '';
                                   var didinput = '';
                                   var doinginput = '';
                                   var statusStr = {
                                      		'wish' : '我想',
                                      		'doing' : '我在',
                                      		'did' : '我过'
                                          };
                                   if (status == 'wish'){
                                       wishinput = 'checked';
                                       donginput = '';
                                       $('show_div_inbox').style.display = 'none';
                                   }else{
                                       if(status == 'did'){
                                    	   didinput= 'checked';
                                       }else{
                                    	   doinginput= 'checked';
                                       }
                                       $('show_div_inbox').style.display = '';
                                   }

                                   if( re.error == -2 ){
                                       $('boxtitle').innerHTML = statusStr[status];
                                   }

                                   var radioDivshow = '<label><input id="wishinput" type="radio" value="wish" name="status"  onclick="$(\'show_div_inbox\').style.display=\'none\';$(\'statushidden\').value=\'wish\';" '+wishinput+'>&nbsp;想 </label>&nbsp;&nbsp;';
                                   radioDivshow += '<label><input id="doinginput" type="radio" value="doing" name="status"  onclick="$(\'show_div_inbox\').style.display=\'\';$(\'statushidden\').value=\'doing\';" '+doinginput+'>&nbsp;在 </label>&nbsp;&nbsp;';
                                   radioDivshow += '<label><input id="didinput" type="radio" value="did" name="status" onclick="$(\'show_div_inbox\').style.display=\'\';$(\'statushidden\').value=\'did\';" '+didinput+'>&nbsp;过 </label>&nbsp;&nbsp;';
                                   radioDivshow += '<a style="color: rgb(102, 102, 102);" href="#" onclick="collect_remove();return false;">删除</a>';
                                   radioDivshow += '<input type="hidden" value="did" name="statushidden" id="statushidden"/><input type="hidden" value="0" name="scorehidden" id="scorehidden"/>';
                                   score = score || 0;

                                   var show_div_inbox = '<span class="stat_post" id="show_score_div_inbox" onmouseout="collect_show_score_inbox('+score+')" style="cursor: pointer;">';
                                   for(var i=1;i<=5;i++){
								   		show_div_inbox += '<span class="stat '+(score>=i?'hover':'')+'" onmouseover="collect_show_score_inbox('+i+')" onclick="$(\'show_score_div_inbox\').onmouseout=collect_show_score_inbox('+i+');">☆</span>';
                                   }
                                   show_div_inbox += '</span>  <span id ="dialog_show_score_description"></span>';
									if(_issinat == true){
										$('sinatdiv').innerHTML = '<input value="1" name="sinatinbox" '+(_iscollect_base2sinat == true ? 'checked="checked"':'' )+' id="collect_sinat_collect_base" style="margin-right: 3px;" type="checkbox"><label for="collect_sinat_collect_base">同步到新浪微博</label>';
									}

                                   $('radioDivScoreBox').innerHTML = radioDivshow;
                                   $('show_div_inbox').innerHTML = show_div_inbox;

                                   path_to_root_dir = "http://v4.vcimg.com/";
                                   var favBox = new GlassBox();
                                   favBox.init( 'showdialog', '300px', '300px', 'false', 'facebookstyle', true, false );
                                   favBox.lbo( false, 0.5 );
                                   favBox.apos($("collectli").cumulativeOffset().left-14+'px',$("collectli").cumulativeOffset().top+12+'px');

                                  }else{
                                      verycdlogin(formHtmlwithoutContinue);
                                      return false;
                                  }
                              },
                          onFailure:function(){
                                return false;
                          }
                       });


                    }

                    function ScoreBoxsubmit(obj){
                        if($('entryid') == undefined){
                            return false;
                        }
                        var entryid = $('entryid').value;
                        var postword = 'entry_id='+entryid+'&';
                        if(obj['statushidden']!= undefined){
                             var status = obj['statushidden']['value'];
                        }else{
                             var status = obj;
                        	if($('sinat_collect_base') && $('sinat_collect_base').checked == true){
                        		postword += 'sinat=1&';
                            }
                        }
                        if(status != 'wish' && status != 'did' && status != 'doing'){
                            return false;
                        }
                        postword += 'status='+status;
                        if(obj['contents'] != undefined){
                            var contents = obj['contents']['value'];
                            postword += '&contents='+contents;
                        }
                        if(obj['scorehidden'] != undefined){
                            var score = obj['scorehidden']['value'];
                            postword += '&score='+score;
                        }else{
                            score = 0;
                         }
                        if(obj['sinatinbox']!= undefined && obj['sinatinbox'].checked == true ){
                       		postword += '&sinat=1';
                        }
                        var thisUrl = '/base/ajax/collect/post';
                        new Ajax.Request (thisUrl, {
                            method:'post',
                            parameters: postword,

                            onComplete: function(transport){
                                var re = YAHOO.lang.JSON.parse(transport.responseText);
                                if (re.code == 0 ) {
                                	score = score||0;
                                	change_html(status,score);
                                    closeScoreBox();
                                    return false;

                                } else if (re.code == -1) { // not logined
                                    closeScoreBox();
                                    verycdlogin(formHtmlwithoutContinue);

                             }
                            }
                        });


                    }
                    function collect_remove() {
                         if($('entryid') == undefined){
                                return false;
                            }
                         var entryid = $('entryid').value;
                         var thisUrl = '/entries/'+entryid+'/collect/remove';
                         new Ajax.Request (thisUrl, {
                                method:'post',
                                parameters: '',
                                onComplete: function(transport){
                                    var re = YAHOO.lang.JSON.parse(transport.responseText);
                                    var storeDoHtml = '';
                                    if (re.code == 0 ) {
										change_html('',0);
                                    	closeScoreBox();
                                    } else if (re.code == -1) { // not logined
                                        closeScoreBox();
                                        verycdlogin(formHtmlwithoutContinue);

                                 }
                                }
                            });
                    }

                </script>

                <script type="text/javascript">
                function can_show_collection_tips() {
                	if( VeryCD.cookie.get('collectentry') ){
                		return false;
                	}
                	if(!VeryCD.member.id>0){
                		$('collectiontips').style.display='';
                		return false;
                	}
                    new Ajax.Request ('/base/api/member/hasCollection', {
                        method:'get',
                        parameters: '',
                        onComplete: function(transport){
                            if (transport.responseText == 1 ) {
                                var Then = new Date();
                                Then.setFullYear(Then.getFullYear() + 2);
                            	VeryCD.cookie.set('collectentry', '1', Then, '/', '.verycd.com');
                    			$('collectiontips').style.display='none';
                            } else {
                            	$('collectiontips').style.display='';
                        	}
                        }
                    });


                }

                function hidden_collection_tips(){
                    var Then = new Date();
                    Then.setFullYear(Then.getFullYear() + 2);
                	VeryCD.cookie.set('collectentry', '1', Then, '/', '.verycd.com');
                	$('collectiontips').style.display='none';
                }
                </script>


          


